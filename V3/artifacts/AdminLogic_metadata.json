		"devdoc": {
			"details": "This contract is designed to be called via delegatecall from a main loan contract,      so it does not maintain its own storage context for loans or parameters.",
			"kind": "dev",
			"methods": {
				"CollateralRatioPercent(uint16)": {
					"details": "Reverts if p == 0 or above 30.00% (3000 bps).",
					"params": {
						"p": "New collateral ratio in basis points."
					}
				},
				"ManagementWallet(address)": {
					"details": "Reverts if w is the zero address or this contract's own address.",
					"params": {
						"w": "The new management wallet address."
					}
				},
				"MaxNPLPercent(uint16)": {
					"details": "Reverts if p is above 50.00% (5000 bps).",
					"params": {
						"p": "New maximum NPL ratio in basis points."
					}
				},
				"MonthlyInterestPercent(uint16)": {
					"details": "Reverts if the interest is zero or above 20.00% (2000 bps).",
					"params": {
						"p": "New interest rate in basis points (e.g., 1000 = 10.00%)."
					}
				},
				"PenaltyPercent(uint16)": {
					"details": "Reverts if the penalty is zero or above 50.00% (5000 bps).",
					"params": {
						"p": "New penalty percentage in basis points."
					}
				},
				"TP()": {
					"details": "Reverts if no profit is available or transfer fails."
				}
			},
			"title": "AdminLogic",
			"version": 1
		},
		"userdoc": {
			"kind": "user",
			"methods": {
				"CollateralRatioPercent(uint16)": {
					"notice": "Sets the required collateral ratio."
				},
				"ManagementWallet(address)": {
					"notice": "Sets the management wallet where protocol profits will be transferred."
				},
				"MaxNPLPercent(uint16)": {
					"notice": "Sets the maximum Non-Performing Loan (NPL) ratio."
				},
				"MonthlyInterestPercent(uint16)": {
					"notice": "Sets the monthly interest rate percentage."
				},
				"PenaltyPercent(uint16)": {
					"notice": "Sets the penalty percentage for missed payments."
				},
				"TP()": {
					"notice": "Transfers all accumulated protocol profits to the management wallet."
				},
				"loans(uint256)": {
					"notice": "Array storing all loans."
				},
				"monthlyInterestPercent()": {
					"notice": "Protocol configuration parameters."
				},
				"totalProfits()": {
					"notice": "Total accumulated profits within the protocol."
				}
			},
			"notice": "Contains administrative functions and configuration for loan protocol parameters,         including interest rates, penalties, collateral ratios, profit management, and wallet settings.",
